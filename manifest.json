{
    "manifest_version": 3,
    "name": "Page Block",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "./images/pageLockIcon.png"
    }
  }
  